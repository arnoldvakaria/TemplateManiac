<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<xs:complexType name="ScriptingItemBase">
		<xs:attribute use="optional" name="groupFile" type="xs:string" />
		<xs:attribute use="optional" name="name" type="xs:string" />
	</xs:complexType>
	<xs:complexType name="ExternalScript">
		<xs:complexContent>
			<xs:extension base="ScriptingItemBase">
				<xs:attribute use="optional" name="fileNamePattern"
					type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="external-script" type="ExternalScript" />
	<xs:complexType name="IncludeScript">
		<xs:complexContent>
			<xs:extension base="ScriptingItemBase">
				<xs:attribute use="required" name="fileName"
					type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="include-script" type="IncludeScript" />
	<xs:complexType name="IncludeDynamicScript">
		<xs:complexContent>
			<xs:extension base="ScriptingItemBase">
				<xs:attribute use="required" name="contentPattern"
					type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="include-dynamic-script" type="IncludeDynamicScript" />
	<xs:complexType name="DynamicExtension">
		<xs:complexContent>
			<xs:extension base="ScriptingItemBase">
				<xs:attribute use="required" name="expressionPattern"
					type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="dynamic-extension" type="DynamicExtension" />
	<xs:complexType name="GeneratorItemBase">
		<xs:complexContent>
			<xs:extension base="ScriptingItemBase">
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="GeneratorContainer">
		<xs:complexContent>
			<xs:extension base="GeneratorItemBase">
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="hql-data-source" />
					<xs:element ref="sql-data-source" />
					<xs:element ref="file-data-source" />
					<xs:element ref="directory-data-source" />
					<xs:element ref="result-transformer" />
					<xs:element ref="data-source-iterator" />
					<xs:element ref="data-source-alias" />
					<xs:element ref="content" />
					<xs:element ref="dynamic-content" />
					<xs:element ref="file-generator" />
					<xs:element ref="list-generator" />
					<xs:element ref="set-generator" />
					<xs:element ref="map-generator" />
					<xs:element ref="formatter" />
					<xs:element ref="message" />
					<xs:element ref="matcher-container" />
					<xs:element ref="include-script" />
					<xs:element ref="include-dynamic-script" />
					<xs:element ref="script-container" />
					<xs:element ref="generator-container" />
				</xs:choice>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="generator-container" type="GeneratorContainer" />
	<xs:complexType name="GeneratorTargetBase">
		<xs:complexContent>
			<xs:extension base="GeneratorContainer">
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DataGeneratorBase">
		<xs:complexContent>
			<xs:extension base="GeneratorTargetBase">
				<xs:attribute use="optional" name="keyPattern"
					type="xs:string" />
				<xs:attribute use="optional" name="clearOnExecution"
					type="xs:boolean" />
				<xs:attribute use="optional" name="inheritTarget"
					type="xs:boolean" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:simpleType name="SetGeneratorDataKind">
		<xs:restriction base="xs:string">
			<xs:enumeration value="HashSet"/>
			<xs:enumeration value="TreeSet"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="SetGenerator">
		<xs:complexContent>
			<xs:extension base="DataGeneratorBase">
				<xs:attribute use="optional" name="splitByNewLines"
					type="xs:boolean" />
				<xs:attribute use="optional" name="kind"
					type="SetGeneratorDataKind" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="set-generator" type="SetGenerator" />
	<xs:complexType name="ListGenerator">
		<xs:complexContent>
			<xs:extension base="DataGeneratorBase">
				<xs:attribute use="optional" name="splitByNewLines"
					type="xs:boolean" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="list-generator" type="ListGenerator" />
	<xs:complexType name="MapGenerator">
		<xs:complexContent>
			<xs:extension base="DataGeneratorBase">
				<xs:attribute use="optional" name="duplicate" 
					type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="map-generator" type="MapGenerator" />
	<xs:element name="on-page-start" type="GeneratorContainer" />
	<xs:element name="on-page-end" type="GeneratorContainer" />
	<xs:complexType name="FileGenerator">
		<xs:complexContent>
			<xs:extension base="GeneratorTargetBase">
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="on-page-start" />
					<xs:element ref="on-page-end" />
				</xs:choice>
				<xs:attribute use="optional" name="fileNamePattern"
					type="xs:string" />
				<xs:attribute use="optional" name="encoding"
					type="xs:string" />
				<xs:attribute use="optional" name="noLock"
					type="xs:boolean" />
				<xs:attribute use="optional" name="forceWritable"
					type="xs:boolean" />
				<xs:attribute use="optional" name="pageSizePattern"
					type="xs:string" />
				<xs:attribute use="optional" name="pagedDs"
					type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="file-generator" type="FileGenerator" />
	<xs:element name="pattern" />
	<xs:complexType name="DynamicContent">
		<xs:complexContent>
			<xs:extension base="GeneratorItemBase">
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="pattern" />
				</xs:choice>      
				<xs:attribute use="optional" name="keyPattern"
					type="xs:string" />
				<xs:attribute use="optional" name="formatterKind"
					type="xs:string" />
				<xs:attribute use="optional" name="noContentIfEmpty"
					type="xs:boolean" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="dynamic-content" type="DynamicContent" />
	<xs:element name="text" />
	<xs:complexType name="Content">
		<xs:complexContent>
			<xs:extension base="GeneratorItemBase">
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="text" />
				</xs:choice>      
				<xs:attribute use="optional" name="key"
					type="xs:string" />
				<xs:attribute use="optional" name="formatterKind"
					type="xs:string" />
				<xs:attribute use="optional" name="noContentIfEmpty"
					type="xs:boolean" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="content" type="Content" />
	<xs:simpleType name="MessageKind">
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Debug"/>
					<xs:enumeration value="Info"/>
					<xs:enumeration value="Warning"/>
					<xs:enumeration value="Error"/>
					<xs:enumeration value="Fatal"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string"/>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:complexType name="Message">
		<xs:complexContent>
			<xs:extension base="ScriptingItemBase">
				<xs:attribute use="optional" name="textPattern"
					type="xs:string" />
				<xs:attribute use="optional" name="kind"
					type="MessageKind" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="message" type="Message" />
	<xs:complexType name="ScriptContainerBase">
		<xs:complexContent>
			<xs:extension base="ScriptingItemBase">
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="pattern" />
					<xs:element ref="hql-data-source" />
					<xs:element ref="sql-data-source" />
					<xs:element ref="file-data-source" />
					<xs:element ref="directory-data-source" />
					<xs:element ref="data-source-iterator" />
					<xs:element ref="data-source-alias" />
					<xs:element ref="context-data-source" />
					<xs:element ref="external-script" />
					<xs:element ref="include-script" />
					<xs:element ref="include-dynamic-script" />
					<xs:element ref="dynamic-extension" />
					<xs:element ref="result-transformer" />
					<xs:element ref="formatter" />
					<xs:element ref="message" />
					<xs:element ref="reloadable-item"/>
					<xs:element ref="template" />
					<xs:element ref="matcher-container" />
					<xs:element ref="script-container" />
					<xs:element ref="generator-container" />
				</xs:choice>      
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DataSourceBase">
		<xs:complexContent>
			<xs:extension base="ScriptContainerBase">
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Formatter">
		<xs:complexContent>
			<xs:extension base="DataSourceBase">
				<xs:attribute use="optional" name="expressionPattern"
					type="xs:string" />
				<xs:attribute use="optional" name="kind"
					type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="formatter" type="Formatter" />
	<xs:complexType name="QueryDataSource">
		<xs:complexContent>
			<xs:extension base="DataSourceBase">
				<xs:attribute use="optional" name="queryPattern"
					type="xs:string" />
				<xs:attribute use="optional" name="iterative"
					type="xs:boolean" />
				<xs:attribute use="optional" name="database"
					type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="HqlDataSource">
		<xs:complexContent>
			<xs:extension base="QueryDataSource">
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="hql-data-source" type="HqlDataSource" />
	<xs:complexType name="SqlDataSource">
		<xs:complexContent>
			<xs:extension base="QueryDataSource">
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="sql-data-source" type="SqlDataSource" />
	<xs:complexType name="ContextDataSource">
		<xs:complexContent>
			<xs:extension base="DataSourceBase">
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="context-data-source" type="ContextDataSource" />
	<xs:complexType name="DataSourceIterator">
		<xs:complexContent>
			<xs:extension base="DataSourceBase">
				<xs:attribute use="required" name="dsNamesPattern"
					type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="data-source-iterator" type="DataSourceIterator" />
	<xs:complexType name="DataSourceAlias">
		<xs:complexContent>
			<xs:extension base="DataSourceBase">
				<xs:attribute use="optional" name="dsNamePattern"
					type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="data-source-alias" type="DataSourceAlias" />
	<xs:complexType name="ScriptContainer">
		<xs:complexContent>
			<xs:extension base="ScriptContainerBase">
				<xs:attribute use="optional" name="database"
					type="xs:string" />
				<xs:attribute use="optional" name="bscFile"
					type="xs:string" />
				<xs:attribute use="optional" name="inheritTarget"
					type="xs:boolean" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="script-container" type="ScriptContainer" />
	<xs:complexType name="FileDataSource">
		<xs:complexContent>
			<xs:extension base="DataSourceBase">
				<xs:attribute use="required" name="fileNamePattern"
					type="xs:string" />
				<xs:attribute use="optional" name="byLines"
					type="xs:boolean" />
				<xs:attribute use="optional" name="encoding"
					type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="file-data-source" type="FileDataSource" />
	<xs:complexType name="DirectoryDataSource">
		<xs:complexContent>
			<xs:extension base="DataSourceBase">
				<xs:attribute use="required" name="fileNamePattern"
					type="xs:string" />
				<xs:attribute use="optional" name="iterative"
					type="xs:boolean" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="directory-data-source" type="DirectoryDataSource" />
	<xs:complexType name="ResultTansformer">
		<xs:complexContent>
			<xs:extension base="DataSourceBase">
				<xs:attribute use="required" name="kind"
					type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="result-transformer" type="ResultTansformer" />
	<xs:simpleType name="FormulaProcessorKindType">
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="ResetAll"/>
					<xs:enumeration value="ResetScript"/>
					<xs:enumeration value="ResetScript4TR"/>
					<xs:enumeration value="ResetScript4ER"/>
					<xs:enumeration value="ResetRule"/>
					<xs:enumeration value="Incremental"/>			
					<xs:enumeration value="IncrementalJustCached"/>
					<xs:enumeration value="IncrementalJustNoComment"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string"/>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:complexType name="FormulaProcessor">
		<xs:complexContent>
			<xs:extension base="DataSourceBase">
				<xs:attribute use="optional" name="kind"
					type="FormulaProcessorKindType" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="formula-processor" type="FormulaProcessor" />
	<xs:complexType name="FormulaProcessorRuleCacher">
		<xs:complexContent>
			<xs:extension base="GeneratorItemBase">
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="formula-processor-rule-cacher" type="FormulaProcessorRuleCacher" />
	<xs:simpleType name="FormulaItemContentChoosingStrategy">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Automatic"/>
			<xs:enumeration value="LongDescriptionWhenNoComment"/>
			<xs:enumeration value="ShortDescriptionWhenNoComment"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="FormulaItem">
		<xs:complexContent>
			<xs:extension base="GeneratorItemBase">
				<xs:choice minOccurs="1" maxOccurs="1">
					<xs:element ref="matcher-container" />
					<xs:element ref="script-container" />
				</xs:choice>
				<xs:attribute use="optional" name="contentChoosing"
					type="FormulaItemContentChoosingStrategy" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="formula-item" type="FormulaItem" />
	<xs:complexType name="FormulaMatcherGenerator">
		<xs:complexContent>
			<xs:extension base="GeneratorItemBase">
				<xs:attribute use="required" name="expressionPattern"
					type="xs:string" />
				<xs:attribute use="required" name="matcherTemplates"
					type="xs:string" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="formula-matcher-generator" type="FormulaMatcherGenerator" />
	<xs:complexType name="ScriptingTemplate">
		<xs:complexContent>
			<xs:extension base="ScriptingItemBase">
				<xs:choice minOccurs="1" maxOccurs="1">
					<xs:element ref="matcher-container" />
					<xs:element ref="script-container" />
				</xs:choice>
				<xs:attribute use="optional" name="maxRecursivityLevel"
					type="xs:integer" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="template" type="ScriptingTemplate" />
	<xs:complexType name="ReloadableItem">
		<xs:complexContent>
			<xs:extension base="ScriptingItemBase">
				<xs:choice minOccurs="1" maxOccurs="1">
					<xs:element ref="script-container" />
				</xs:choice>      
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="reloadable-item" type="ReloadableItem" />
	<xs:complexType name="MatcherItemBase">
		<xs:complexContent>
			<xs:extension base="GeneratorItemBase">
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="on-match" />
					<xs:element ref="on-after-match" />
					<xs:element ref="on-partial-match" />
				</xs:choice>
				<xs:attribute use="optional" name="expressionPattern"
					type="xs:string" />
				<xs:attribute use="optional" name="displayName"
					type="xs:string" />
				<xs:attribute use="optional" name="eraseEvents"
					type="xs:boolean" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="MatcherScriptContainer">
		<xs:complexContent>
			<xs:extension base="ScriptContainerBase">
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="on-match" type="MatcherScriptContainer" />
	<xs:element name="on-after-match" type="MatcherScriptContainer" />
	<xs:element name="on-partial-match" type="MatcherScriptContainer" />
	<xs:complexType name="MatcherContainerBase">
		<xs:complexContent>
			<xs:extension base="MatcherItemBase">
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element ref="any-char" />
					<xs:element ref="char-in-string" />
					<xs:element ref="char-not-in-string" />
					<xs:element ref="empty" />
					<xs:element ref="end-of-parse" />
					<xs:element ref="start-of-parse" />
					<xs:element ref="not-end-of-parse" />
					<xs:element ref="fix-nr-of-times" />
					<xs:element ref="match-one-of-them" />
					<xs:element ref="no-digit" />
					<xs:element ref="no-white" />
					<xs:element ref="null-or-many-multi" />
					<xs:element ref="match-one-after-another" />
					<xs:element ref="one-digit" />
					<xs:element ref="one-or-many-multi" />
					<xs:element ref="one-white" />
					<xs:element ref="string-match" />
					<xs:element ref="string-pattern-match" />
					<xs:element ref="byte-match" />
					<xs:element ref="word-by-word" />
					<xs:element ref="is-in-collection" />
					<xs:element ref="reg-exp"/>
					<xs:element ref="until-the-string"/>
					<xs:element ref="optional-matching"/>
					<xs:element ref="matcher-reference"/>
					<xs:element ref="dynamic-matcher"/>
					<xs:element ref="matcher-timer"/>
					<xs:element ref="antlr-matcher"/>
					<xs:element ref="matcher-with-target"/>
					<xs:element ref="forward-no-match"/>
					<xs:element ref="forward-match"/>
					<xs:element ref="pair-match"/>
				</xs:choice>      
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="MatcherContainmentBase">
		<xs:complexContent>
			<xs:extension base="MatcherItemBase">
				<xs:choice minOccurs="1" maxOccurs="1">
					<xs:element ref="any-char" />
					<xs:element ref="char-in-string" />
					<xs:element ref="char-not-in-string" />
					<xs:element ref="empty" />
					<xs:element ref="end-of-parse" />
					<xs:element ref="start-of-parse" />
					<xs:element ref="not-end-of-parse" />
					<xs:element ref="fix-nr-of-times" />
					<xs:element ref="match-one-of-them" />
					<xs:element ref="no-digit" />
					<xs:element ref="no-white" />
					<xs:element ref="null-or-many-multi" />
					<xs:element ref="match-one-after-another" />
					<xs:element ref="one-digit" />
					<xs:element ref="one-or-many-multi" />
					<xs:element ref="one-white" />
					<xs:element ref="string-match" />
					<xs:element ref="string-pattern-match" />
					<xs:element ref="byte-match" />
					<xs:element ref="word-by-word" />
					<xs:element ref="is-in-collection" />
					<xs:element ref="reg-exp"/>
					<xs:element ref="until-the-string"/>
					<xs:element ref="optional-matching"/>
					<xs:element ref="matcher-reference"/>
					<xs:element ref="dynamic-matcher"/>
					<xs:element ref="matcher-timer"/>
					<xs:element ref="antlr-matcher"/>
					<xs:element ref="matcher-with-target"/>
					<xs:element ref="forward-no-match"/>
					<xs:element ref="forward-match"/>
					<xs:element ref="pair-match"/>
				</xs:choice>      
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="MatcherContainer">
		<xs:complexContent>
			<xs:extension base="MatcherContainmentBase">
				<xs:attribute use="optional" name="useBacktracking"
					type="xs:boolean" />
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="matcher-container" type="MatcherContainer" />
	<xs:complexType name="MatcherWithTarget">
		<xs:complexContent>
			<xs:extension base="MatcherContainmentBase">
				<xs:choice minOccurs="0" maxOccurs="1">
					<xs:element ref="list-generator" />
					<xs:element ref="map-generator" />
				</xs:choice>      
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="matcher-with-target" type="MatcherWithTarget" />
	<xs:complexType name="ForwardNoMatch">
		<xs:complexContent>
			<xs:extension base="MatcherContainmentBase">
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="forward-no-match" type="ForwardNoMatch" />
	<xs:complexType name="ForwardMatch">
		<xs:complexContent>
			<xs:extension base="MatcherContainmentBase">
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="forward-match" type="ForwardMatch" />
	<xs:complexType name="PairMatch">
		<xs:complexContent>
			<xs:extension base="MatcherContainerBase">
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="pair-match" type="PairMatch" />
	<xs:complexType name="AnyChar">
		<xs:complexContent>
			<xs:extension base="MatcherItemBase">
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="any-char" type="AnyChar" />
	<xs:complexType name="Empty">
		<xs:complexContent>
			<xs:extension base="MatcherItemBase">
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
