<?xml version="1.0" encoding="utf-8"?>
<script-container groupFile="Test.stg"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../script.xsd">
	<generator-container>
		<list-generator name="testListA">
			<dynamic-content>
				<pattern>A</pattern>
				<pattern>B</pattern>
				<pattern>C</pattern>
			</dynamic-content>
			<dynamic-content>
				<pattern></pattern>
			</dynamic-content>
		</list-generator>
	</generator-container>
	<generator-container>
		<list-generator name="testListB">
			<dynamic-content>
				<pattern>A</pattern>
				<pattern>B</pattern>
				<pattern>C</pattern>
			</dynamic-content>
			<dynamic-content noContentIfEmpty="true">
				<pattern></pattern>
			</dynamic-content>
		</list-generator>
	</generator-container>
	<message textPattern='$ds.all.testListA.result:{$it$};separator=","$'/>
	<message textPattern='$ds.all.testListB.result:{$it$};separator=","$'/>
</script-container>